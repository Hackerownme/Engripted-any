<script type="text/javascript">
function decode(v){
    return unescape(v.replace(/\\u00/g, '%'));
}
function encode(v){
    v = v.split('');
    for(var i = 0; i < v.length; ++i){
        v[i] = v[i].charCodeAt(0).toString(16).toUpperCase();
        if(v[i].length < 2){v[i] = '0' + v[i];}
    }
    return '\\u00' + v.join('\\u00');
}
</script>
